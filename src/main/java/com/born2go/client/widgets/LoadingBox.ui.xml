<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .LoadingBox-Obj1;
	.LoadingBox-Obj1 {
		background-color: #f6f7f8;
		border: 1px solid #a1a1a1;
		border-radius: 5px;
		-webkit-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		-moz-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
	}
	
	.LoadingBox-Obj2 {
		margin: auto;
		position: relative;
		top: 20px;
	}
	
	.LoadingBox-Obj3 {
		position: relative;
		top: -30px;
		margin: auto;
	}
	
	.LoadingBox-Obj5 {
		position: absolute;
		top: 74px;
		right: 45px;
		font-size: 15px;
	}
	</ui:style>
	<g:HTMLPanel height="190px" width="280px">
		<g:HTMLPanel width="100px" height="100px"
			styleName="{style.LoadingBox-Obj2}">
			<g:Image url="/resources/1427111517_palm_tree.png" width="100%"
				height="100%" />
		</g:HTMLPanel>
		<g:HTMLPanel width="240px" height="160px"
			styleName="{style.LoadingBox-Obj3}">
			<g:Image url="/resources/loading2.gif" width="100%" height="100%"/>
			<g:Label ui:field="uploadPercent" styleName="{style.LoadingBox-Obj5}" visible="false">20%</g:Label>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 