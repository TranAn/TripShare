<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .LoginDialog-Obj1;
	.LoginDialog-Obj1 {
		background: #f6f7f8;
		opacity: 0;
		border: 1px solid #a1a1a1;
		-webkit-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		-moz-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
	}
	
	.LoginDialog-Obj2 {
		padding: 20px;
	}
	
	.LoginDialog-Obj3 {
		float: left;
	}
	
	.LoginDialog-Obj4 {
		float: right;
		cursor: pointer;
	}
	
	.LoginDialog-Obj5 {
		cursor: pointer;
		margin: auto;
		margin-top: 75px;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.LoginDialog-Obj2}" height="200px" width="400px">
		<g:HTMLPanel>
			<g:Label styleName="font-blackTitleNormal {style.LoginDialog-Obj3}">Login Require</g:Label>
			<g:Anchor ui:field="btnExit" styleName="{style.LoginDialog-Obj4}">&laquo;Go Back</g:Anchor>
		</g:HTMLPanel>
		<g:HTMLPanel>
			<g:Image ui:field="btnLogin" styleName="{style.LoginDialog-Obj5}" url="/resources/f-login.png"></g:Image>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 