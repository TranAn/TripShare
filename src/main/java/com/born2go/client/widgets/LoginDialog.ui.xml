<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .LoginDialog-Obj1;
	.LoginDialog-Obj1 {
		background: #f6f7f8;
		opacity: 0;
		border: 1px solid #a1a1a1;
		-webkit-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		-moz-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
	}
	
	.LoginDialog-Obj2 {
		padding: 20px;
	}
	
	.LoginDialog-Obj3 {
		font-size: 16px;
		text-align: center;
		margin-bottom: 40px;
	}
	
	.LoginDialog-Obj4 {
		cursor: pointer;
		font-size: 14px;
		margin-left: 77px;
	}
	
	.LoginDialog-Obj5 {
		cursor: pointer;
		margin-bottom: 20px;
	}
	
	.LoginDialog-Obj6 {
		width: 205px;
		margin: auto;
		margin-top: 5px;
	}
	
	.LoginDialog-Obj7 {
		margin: auto;
		margin-bottom: 5px;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.LoginDialog-Obj2}" height="220px"
		width="250px">
		<g:HTMLPanel styleName="{style.LoginDialog-Obj6}">
			<div style="width: 60px; margin: auto;">
				<g:Image width="60px" styleName="{style.LoginDialog-Obj7}"
					url="/resources/1427111517_palm_tree.png"></g:Image>
			</div>
			<g:Label styleName="font-blackTitleNormal {style.LoginDialog-Obj3}">Login Require</g:Label>
			<div>
				<g:Image ui:field="btnLogin" styleName="{style.LoginDialog-Obj5}"
					url="/resources/f-login.png"></g:Image>
			</div>
			<g:Anchor ui:field="btnExit" styleName="{style.LoginDialog-Obj4}">Cancel</g:Anchor>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 