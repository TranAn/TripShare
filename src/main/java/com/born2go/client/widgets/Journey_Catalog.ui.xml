<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .Catalog_Obj1, Catalog_Obj1_Open, .Catalog_Obj4, .Catalog_Obj5, .Catalog_Obj6, .Catalog_Obj7;
	.Catalog_Obj1 {
		margin-top: 5px;
		position: relative;
		border-top: none;
		background-color: #FCFFFF;
		border-radius: 0px 0px 10px 10px;
		height: auto !important;
		overflow-y: hidden;
		max-height: 0px;
		transition-property: all;
		transition-duration: .5s;
		transition-timing-function: cubic-bezier(0, 1, 0.5, 1);
	}
	
	.Catalog_Obj1_Open {
		max-height: 450px;
	}
	
	.Catalog_Obj2 {
		background-color: #f6f7f8;
		border: 1px #e9eaed solid;
		padding: 15px;
	}
	
	.Catalog_Obj3 {
		background-color: #ffffff;
		padding: 10px;
		padding-bottom: 80px;
		position: relative;
	}
	
	.Catalog_Obj4 {
		font-family: museo-w01-700, serif;
		font-size: 15px;
		padding: 9px;
		cursor: pointer;
	}
	
	.Catalog_Obj4:hover img {
		opacity: 1.0 !important;
	}
	
	.Catalog_Obj4:hover .Catalog_Obj5 {
		color: black;
	}
	
	.Catalog_Obj5 {
		color: gray;
	}
	
	.Catalog_Obj5:hover {
		text-decoration: underline;
		color: black;
	}
	
	.Catalog_Obj6 {
		display: inline-flex;
		display: -webkit-inline-box;
		display: -webkit-inline-flex;
		display: -ms-inline-flexbox;
		-webkit-align-self: auto;
		margin-left: 6px;
		margin-top: 10px;
		margin-bottom: 10px;
	}
	
	.Catalog_Obj7 {
		margin-left: 10px;
		font-weight: 500;
		font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
		font-size: 15px;
		position: relative;
		top: -2px
	}
	
	.Catalog_Obj8 {
		float: right;
		font-weight: 700;
		font-family: "Helvetica Neue", Arial, Helvetica, sans-serif;
		font-size: 13px;
		color: silver;
		position: relative;
		top: -2px;
	}
	
	.Catalog_Obj9 {
		position: absolute;
		opacity: 0.4;
		top: 20px;
		right: 10px;
		width: 260px;
	}
	</ui:style>
	<g:HTMLPanel ui:field="catalog" styleName="{style.Catalog_Obj1}"
		height="200px">
		<g:HTMLPanel height="16px" styleName="{style.Catalog_Obj2}"
			width="">
			<g:Label text="Book of content" styleName="font-blackTitleNormal" />
		</g:HTMLPanel>

		<g:ScrollPanel height="360px">
			<g:HTMLPanel ui:field="htmlContent" styleName="{style.Catalog_Obj3}">
				<g:Image url="/resources/food-diary-resize.jpg" styleName="{style.Catalog_Obj9}"></g:Image>
			</g:HTMLPanel>
		</g:ScrollPanel>

		<g:HTMLPanel height="10px" styleName="{style.Catalog_Obj2}">
			<g:Label styleName="{style.Catalog_Obj8}" ui:field="totalPost"></g:Label>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 