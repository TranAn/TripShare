<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	@external .PathView-Obj3, .PathView-Obj4, .PathView-Obj13, .PathView-Obj14;
	.PathView-Obj1 {
		margin: auto;
		padding: 20px 0px;
		padding-bottom: 35px;
	}
	
	.PathView-Obj2 {
		color: aliceblue;
  		padding: 6px 10px;
  		margin-left: 10px;
	}
	
	.PathView-Obj3 {
		margin-top: 30px;
  		margin-bottom: 50px;
  		border: 2px solid #a1a1a1;
		background-color: #f6f7f8;
		border-radius: 10px;
		-webkit-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		-moz-box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
		box-shadow: 1px 1px 2px 1px rgba(0, 0, 0, 0.25);
	}
	
	.PathView-Obj4 {
		position: fixed;
		width: 1020px;
		top: 0px;
		border: 2px solid #a1a1a1;
		border-top: none;
		background-color: rgba(255, 254, 252, 0.81);
		border-radius: 0px 0px 10px 10px;
		-webkit-box-shadow: 0 4px 2px -2px rgba(0, 0, 0, 0.25);
		-moz-box-shadow: 0 4px 2px -2px rgba(0, 0, 0, 0.25);
		box-shadow: 0 4px 2px -2px rgba(0, 0, 0, 0.25);
		z-index: 1;
	}
	
	.PathView-Obj5 {
		float: left;
		margin-left: 15px;
	}
	
	.PathView-Obj6 {
		position: relative;
		top: -5px;
		float: right;
		margin-right: 22px;
	}
	
	.PathView-Obj7 {
		margin-top: 6px;
		margin-right: 10px;
	}
	
	.PathView-Obj8 {
		border: 1px silver solid;
	}
	
	.PathView-Obj9 {
		color: aliceblue;
  		padding: 6px 50px;
  		margin-left: 10px;
	}
	
	.PathView-Obj10 {
		margin: auto;
	}
	
	.PathView-Obj12 {
		margin: auto;
		padding: 20px 0px;
	}
	
	.PathView-Obj13 {
		margin-bottom: 0px;
	}
	
	.PathView-Obj14 {
	  	color: aliceblue;
  		background-color: silver;
	}
	
	.PathView-Obj14:hover {
	  	color: aliceblue !important;
  		background-color: silver;
  		font-weight: normal !important;
	}
	
	.PathView_Obj15 {
	  	width: 1px;
	 	border-right: 1px silver dotted;
	  	height: 15px;
	  	margin-left: 15px;
	  	margin-right: 5px;
	}
	</ui:style>
	<g:HTMLPanel ui:field="pathViewForm">
		<g:HTMLPanel ui:field="dummyNode"></g:HTMLPanel>
		<g:HTMLPanel ui:field="htmlPathToolbar" styleName="{style.PathView-Obj3}">
			<g:HTMLPanel ui:field="toolbar" styleName="{style.PathView-Obj1}">
				<g:HorizontalPanel styleName="{style.PathView-Obj5}">
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="65px" ui:field="btnEdit"><i style="margin-right:5px;" class="fa fa-file-text-o fa-lg"></i> Edit</g:Anchor>
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="65px" ui:field="btnThemes"><i style="margin-right:5px;" class="fa fa-magic fa-lg"></i> Themes</g:Anchor>
					<g:HTMLPanel styleName="{style.PathView_Obj15}"></g:HTMLPanel>
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="65px" ui:field="btnPost"><i style="margin-right:5px;" class="fa fa-pencil fa-lg"></i> Post</g:Anchor>
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="105px" ui:field="btnUpload"><i style="margin-right:5px;" class="fa fa-camera fa-lg"></i> Upload Photos</g:Anchor>
					<g:HTMLPanel styleName="{style.PathView_Obj15}"></g:HTMLPanel>
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="105px" ui:field="btnContent"><i style="margin-right:5px;" class="fa fa-book fa-lg"></i> Catalog</g:Anchor>
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="72px" ui:field="btnGallery"><i style="margin-right:5px;" class="fa fa-picture-o fa-lg"></i> Gallery</g:Anchor>
					<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="72px" ui:field="btnComment"><i style="margin-right:5px;" class="fa fa-comment fa-lg"></i> Comment</g:Anchor>
				</g:HorizontalPanel>
				<g:HorizontalPanel styleName="{style.PathView-Obj6}">
					<g:Label text="Arrange by:" styleName="{style.PathView-Obj7} font-blackTitleNormal" />
					<g:ListBox styleName="gwt-ListBox {style.PathView-Obj8}" height="30px" width="100px" ui:field="listArrange"/>
				</g:HorizontalPanel>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="editToolbar" styleName="{style.PathView-Obj12}" height="" visible="false">
				<g:HorizontalPanel styleName="{style.PathView-Obj10}">
					<g:Anchor ui:field="btnSave"  styleName="greenbutton {style.PathView-Obj9}" ><i style="margin-right:5px;" class="fa fa-floppy-o fa-lg"></i> Save</g:Anchor>
					<g:Anchor ui:field="btnCancel"  styleName="greenbutton {style.PathView-Obj9}" ><i style="margin-right:5px;" class="fa fa-ban fa-lg"></i> Cancel</g:Anchor>
				</g:HorizontalPanel>
			</g:HTMLPanel>
			<g:HTMLPanel ui:field="htmlControlBottom"></g:HTMLPanel>
		</g:HTMLPanel>
	
		<g:FlowPanel ui:field="htmlPathTable"></g:FlowPanel>
		
		<g:HTMLPanel ui:field="facebookComments"></g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 