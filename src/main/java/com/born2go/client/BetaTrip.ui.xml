<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.obj1 {
		padding: 30px;
		background-color: #f6f7f8;
		border-top: 1px silver dotted;
		border-bottom: 1px silver dotted;
	}
	
	.obj2 {
		font-size: 30px;
		font-family: museo-w01-700, serif;
		color: rgb(56, 119, 127);
	}
	
	.obj3 {
		font-size: 20px;
		font-family: museo-w01-700, serif;
		color: silver;
		font-style: italic;
		margin-bottom: 25px;
	}
	
	.obj4 {
		margin: auto;
		width: 100%;
		margin-bottom: 15px;
	}
	
	.obj5 {
		font-size: 14px;
		color: red;
		opacity: 0.9;
	}
	</ui:style>
	<g:HTMLPanel styleName="{style.obj1}" height="130px">
		<g:HTMLPanel styleName="{style.obj4}" width="345px">
			<g:Anchor text="Việt - Lào: Đổ xăng xe và đi" styleName="{style.obj2}" ui:field="link"></g:Anchor>
		</g:HTMLPanel>
		<g:Label text="Create by Vu Quang Hoa" horizontalAlignment="ALIGN_CENTER"
			styleName="{style.obj3}" />
		<g:Label text="Note: Chuyến vượt biên sang thăm nhân dân Lào của Mr.Hòa" horizontalAlignment="ALIGN_CENTER" styleName="{style.obj5}"/>
	</g:HTMLPanel>
</ui:UiBinder> 