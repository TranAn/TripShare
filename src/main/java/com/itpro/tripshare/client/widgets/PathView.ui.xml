<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.PathView-Obj1 {
		margin: auto;
	}
	
	.PathView-Obj2 {
		color: aliceblue;
  		padding: 6px 10px;
  		margin-left: 10px;
	}
	
	.PathView-Obj3 {
		margin-top: 30px;
  		margin-bottom: 60px;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{style.PathView-Obj3}">
			<g:HorizontalPanel styleName="{style.PathView-Obj1}">
				<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="65px" ui:field="btnEdit"><i style="margin-right:5px;" class="fa fa-file-text-o fa-lg"></i> Edit</g:Anchor>
				<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="65px" ui:field="btnPost"><i style="margin-right:5px;" class="fa fa-pencil fa-lg"></i> Post</g:Anchor>
				<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="105px" ui:field="btnUpload"><i style="margin-right:5px;" class="fa fa-camera fa-lg"></i> Upload Photo</g:Anchor>
				<g:Anchor styleName="greenbutton {style.PathView-Obj2}" width="72px" ui:field="btnGallery"><i style="margin-right:5px;" class="fa fa-picture-o fa-lg"></i> Gallery</g:Anchor>
			</g:HorizontalPanel>
		</g:HTMLPanel>
		
		<g:HTMLPanel ui:field="htmlPathCreate"></g:HTMLPanel>
		
		<g:HTMLPanel ui:field="htmlPathTable"></g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder> 